{
    "printWidth": 120,
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always",
    "bracketSpacing": true,
    "quoteProps": "as-needed",
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "vueIndentScriptAndStyle": true,
    "endOfLine": "lf",
    "singleAttributePerLine": true,
    "bracketSameLine": false,
    "embeddedLanguageFormatting": "auto",
    "insertPragma": false,
    "requirePragma": false
}
