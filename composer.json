{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "cheesegrits/filament-google-maps": "^3.0",
        "filament/filament": "^3.0-stable",
        "filament/spatie-laravel-media-library-plugin": "^3.0-stable",
        "flowframe/laravel-trend": "^0.1.5",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.8",
        "irazasyed/telegram-bot-sdk": "^3.13",
        "johngrogg/ics-parser": "^3.3",
        "laravel-notification-channels/telegram": "^4.0",
        "laravel/framework": "^10.10",
        "laravel/jetstream": "^4.1",
        "laravel/sanctum": "^3.3",
        "laravel/socialite": "^5.10",
        "laravel/tinker": "^2.8",
        "maatwebsite/excel": "^3.1",
        "propaganistas/laravel-phone": "^5.0",
        "saade/filament-fullcalendar": "^3.1",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/vkontakte": "^5.0",
        "spatie/laravel-permission": "^6.1",
        "tightenco/ziggy": "^1.0",
        "yoomoney/yookassa-sdk-php": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "barryvdh/laravel-ide-helper": "*",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models -n",
            "@php artisan ide-helper:meta",
            "./vendor/bin/pint"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
